        "storage.type.interface.tsx",
        "storage.type.js",
        "storage.type.jsx",
        "storage.type.namespace.js",
        "storage.type.namespace.jsx",
        "storage.type.namespace.ts",
        "storage.type.namespace.tsx",
        "storage.type.ts",
        "storage.type.tsx",
        "storage.type.type.js",
