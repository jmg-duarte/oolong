  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": {
      "foreground": "#A2D5E0",
      "fontStyle": ""
    },
    "struct": {
      "foreground": "#A2D5E0",
      "fontStyle": ""
    },
    "enum": {
      "foreground": "#A2D5E0",
      "fontStyle": ""
    },
    "enumMember": {
      "foreground": "#A2D5E0",
      "fontStyle": ""
    },
    // Types
    "type": {
      "foreground": "#A2D5E0"
    },
    "typeAlias": {
      "foreground": "#A2D5E0"
    },
    "*.typeHint": {
      "foreground": "#A2D5E0"
    },
    "interface": {
      "foreground": "#A2D5E0"
    },
    // Macro related
    "macro": {
      "foreground": "#C095EF"
    },
    "builtinAttribute": {
      "foreground": "#C095EF"
    },
    "decorator": {
      "foreground": "#C095EF"
    },
    "deriveHelper": {
      "foreground": "#C095EF"
    },
    "attribute": "#C095EF",
    "attributeBracket": "#C095EF",
    // Functions / Methods
    "method": {},
    "function": {},
    "magicFunction": {
      "fontStyle": "italic"
    },
    // Function Parameters
    "parameter": {
      "fontStyle": ""
    },
    "escapeSequence": {
      "foreground": "#C095EF",
      "fontStyle": "bold"
    },
    "keyword": {
      "fontStyle": "",
      "foreground": "#9AE399"
    },
    "namespace": {
      "fontStyle": ""
    },
    "operator": {
      "fontStyle": ""
    },
    "*.controlFlow": {
      "fontStyle": "bold"
    },
    "*.unsafe": {
      "fontStyle": "bold",
      "foreground": "#fe6f6f"
    },
    "*.consuming": {
      "fontStyle": "italic"
    },
    "*.mutable": {
      "fontStyle": "underline"
    },
    // They're usually upper case
    "*.constant": {
      "fontStyle": ""
    },
    "builtinConstant": {
      "fontStyle": ""
    },
    "*.builtin": {
      "fontStyle": ""
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    "comment": "#5bc648"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#5AC648"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.quoted.single.python"
      ],
      "settings": {
        "foreground": "#5FC3A5",
        "fontStyle": "" // Empty to enforce no style
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#C095EF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Constant Literals",
      "scope": ["constant.numeric", "constant.language.json.comments"],
      "settings": {
        "foreground": "#5FC3A5"
      }
    },
    {
      "name": "YAML: True, False, Null",
      "scope": [
        "constant.language.boolean.yaml",
        "constant.language.null.yaml"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Fallback: Enum & Enum Member",
      "scope": ["entity.name.type.enum", "variable.other.enummember"],
      "settings": {
        "foreground": "#A2D5E0",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Namespaces",
      "scope": "entity.name.namespace",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Types",
      "scope": ["entity.name.type", "support.type", "storage.type"],
      "settings": {
        "foreground": "#A2D5E0",
        "fontStyle": ""
      }
    },
    {
      // NOTE: maybe just keyword.control would work
      "name": "Fallback: Control Flow",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Fallback: Struct",
      "scope": ["storage.type.struct"],
      "settings": {
        "foreground": "#A2D5E0",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Class",
      "scope": ["entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#A2D5E0",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#A2D5E0",
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.function.member"
      ],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Fallback: Macro",
      "scope": ["entity.name.function.macro"],
      "settings": {
        "foreground": "#C095EF"
      }
    },
    {
      "name": "Fallback: Variable",
      "scope": [
        "variable.other.readwrite",
        "entity.name.variable",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {}
    },
    {
      "name": "Fallback: Parameter",
      "scope": ["variable.parameter"],
      "settings": {}
    },
    {
      "name": "Fallback: Property",
      "scope": ["variable.parameter", "variable.other.parameter"],
      "settings": {}
    },
    {
      "name": "Fallback: Event",
      "scope": ["variable.other.event"],
      "settings": {}
    },
    {
      "name": "Fallback: Keywords",
      "scope": [
        "keyword.function",
        "keyword.control",
        "keyword.var",
        "keyword.type",
        "keyword.package",
        "keyword.interface"
      ],
      "settings": {
        "foreground": "#9AE399"
      }
    },
    {
      "name": "Python: Decorators",
      "scope": [
        "punctuation.definition.decorator.python",
        "entity.name.function.decorator.python",
        "meta.function.decorator.python"
      ],
      "settings": {
        "foreground": "#C095EF"
      }
    },
    {
      "name": "Python Keyword: Declarations",
      "scope": [
        "constant.language.python",
        "keyword.control.flow.python",
        "keyword.control.import.python",
        "keyword.operator.logical.python",
        "storage.type.class.python",
        "storage.type.function.async.python",
        "storage.type.function.lambda.python",
        "storage.type.function.python"
      ],
      "settings": {
        "foreground": "#9AE399"
      }
    },
    {
      // Necessary because they'll inherit the decorator color
      "name": "Python: Function Call Parameter",
      "scope": ["variable.parameter.function-call.python"],
      "settings": {
        "foreground": "#E3BC8F",
        "fontStyle": ""
      }
    },
    {
      "name": "TS Keyword",
      "scope": [
        "constant.language.boolean.false.js",
        "constant.language.boolean.false.jsx",
        "constant.language.boolean.false.ts",
