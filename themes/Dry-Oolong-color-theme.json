  "semanticHighlighting": true,
  "semanticTokenColors": {
    "magicFunction": {
      "fontStyle": "italic"
    },
    "*.controlFlow": {
      "fontStyle": "bold"
    },
    "*.unsafe": {
      "fontStyle": "bold"
    },
    "*.consuming": {
      "fontStyle": "italic"
    },
    "*.mutable": {
      "fontStyle": "underline"
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    "comment": "#6CCD5B",
    "label": {
      "fontStyle": "bold"
    }
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#6CCD5B"
      }
    },
    {
      "name": "Control Flow",
      "scope": ["keyword.control"],
      "settings": {
        "fontStyle": "bold"
      }
    }
